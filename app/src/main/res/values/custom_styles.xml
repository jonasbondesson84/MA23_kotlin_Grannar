<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Circular">
        <item name="cornerSize">50%</item>
    </style>


    <style name="SmallInterestChip">
        <item name="chipCornerRadius">5dp</item>
        <item name="chipEndPadding">10dp</item>
        <item name="chipMinHeight">20dp</item>
        <item name="chipStartPadding">10dp</item>
        <item name="textEndPadding">0dp</item>
        <item name="textStartPadding">0dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:visibility">invisible</item>

    </style>

    <style name="SmallInterestChip.NonClickable">
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="SmallInterestChip.Icon">
        <item name="chipIcon">@drawable/baseline_close_24</item>
        <item name="chipStartPadding">5dp</item>
    </style>

</resources>